  {
    "name": "Time count",
    "version": "1.0",
    "description": "Sum the time",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }